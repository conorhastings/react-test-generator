{
  "name": "react-test-generator",
  "version": "0.0.0",
  "description": "generate tests for react components with propTypes and static analysis",
  "main": "index.js",
  "scripts": {
    "test": "tape ./test/*.js",
    "build": "./node_modules/.bin/webpack --progress",
    "gen-test": "babel test --out-dir gen-test"
  },
  "keywords": [
    "tests",
    "tape",
    "react",
    "generate",
    "generator",
    "props",
    "propTypes",
    "static",
    "static-analysis"
  ],
  "author": "Conor Hastings",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.6.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-docgen": "^2.7.0",
    "react-dom": "^0.14.7",
    "tape": "^4.4.0",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "babel-plugin-react-transform": "^2.0.0"
  }
}
